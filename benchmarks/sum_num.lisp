(define (sum-num x y z a b)
    (+ (if (num? x) x 0)
        (+ (if (num? y) y 0)
            (+ (if (num? z) z 0)
                (+ (if (num? a) a 0)
                    (if (num? b) b 0)
                )
            )
        )
    )
)

(let ((q 1))
    (let ((w true))
        (let ((e (add1 30)))
            (let ((r (sub1 60)))
                (let ((t 2))
                    (print (sum-num q w e r t))
                )
            )
        )
    )
)